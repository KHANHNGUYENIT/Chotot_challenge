{
  "expo": {
    "name": "GMApp",
    "description": "app demo for coderschool",
    "slug": "frontend-app",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.jpg",
    "splash": {
      "image": "./assets/images/logoimage.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffd503"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "package": "com.GMapp.GMappDemo"
    },
    "githubUrl": "https://github.com/KHANHNGUYENIT/Chotot_challenge/tree/master/code/react-app/frontend-app"
  }
}
